{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Java",
			"type": "java",
			"request": "launch",
			"stopOnEntry": true,
			"jdkPath": "${env:JAVA_HOME}/bin",
			"cwd": "${workspaceRoot}/src/bubblesort/",
			"startupClass": "bubblesort.BubbleSorter",
			"classpath": [
				"${workspaceRoot}/class/"
			],
			"args": [
				"4",
				"3",
				"2",
				"1"
			]
		},
		{
			"name": "Java Console App",
			"type": "java",
			"request": "launch",
			"stopOnEntry": true,
			"jdkPath": "${env:JAVA_HOME}/bin",
			"cwd": "${fileDirname}",
			"startupClass": "${fileBasenameNoExtension}",
			"classpath": [
				".",
				"${fileDirname}"
			],
			"externalConsole": true
		}
	]
}