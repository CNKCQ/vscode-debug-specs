{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "attach local XDebug",
			"type": "php",
			"request": "launch",
			"port": 9000
		},
		{
			"name": "Launch currently open script",
			"type": "php",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
		},
		{
			"name": "attach remote XDebug",
			"type": "php",
			"request": "launch",
			"server": "192.168.1.24",
			"port": 9000,
			"pathMappings": {
				"/home/nnyn/vscode-debug-specs/php": "${workspaceFolder}"
			}
		},
	]
}