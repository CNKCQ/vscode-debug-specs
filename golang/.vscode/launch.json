{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/bubblesorter/cmd/bubblesorter",
			"args": [
				"4",
				"3",
				"2",
				"1"
			]
		},
		{
			"name": "Launch test function",
			"type": "go",
			"request": "launch",
			"mode": "test",
			"program": "${workspaceRoot}/bubblesorter",
			"args": [
				"-test.run",
				"TestBubbleSort"
			]
		},
		{
			"name": "Attach remote process",
			"type": "go",
			"request": "launch",
			"mode": "remote",
			"remotePath": "${workspaceRoot}/cmd/bubbleSorter",
			"port": 2345,
			"host": "192.168.56.101",
			"program": "${workspaceRoot}/cmd/bubbleSorter",
			"env": {},
			"args": [],
			"showLog": true
		}
	]
}