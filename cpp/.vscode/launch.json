{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Launch Program",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/main",
			"preLaunchTask": "build",
			"args": [
				"4",
				"3",
				"2",
				"1"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb"
		},
		{
			"name": "(lldb) Attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/main",
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		},
		{
			"name": "(lldb) Launch cunit",
